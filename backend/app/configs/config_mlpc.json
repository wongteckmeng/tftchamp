{
    "name": "Regression",
    "model": {
        "type": "Model",
        "args": {
            "pipeline": [
                "column_transformer",
                "MLP"
            ],
            "unions": {}
        }
    },
    "tuned_parameters": [
        {
            "MLP__hidden_layer_sizes": [
                [
                    100, 50, 25, 5
                ]
            ],
            "MLP__learning_rate_init": [
                0.001,
                0.003,
                0.007,
                0.021
            ],
            "MLP__alpha": [
                0.0001,
                0.001,
                0.1,
                0.32,
                1.0,
                3.0
            ],
            "MLP__max_iter": [
                1000
            ],
            "MLP__early_stopping": [
                true
            ]
        }
    ],
    "optimizer": "OptimizerRegression",
    "search_method": {
        "type": "GridSearchCV",
        "args": {
            "refit": false,
            "n_jobs": 1,
            "verbose": 2,
            "error_score": 0
        }
    },
    "cross_validation": {
        "type": "RepeatedStratifiedKFold",
        "args": {
            "n_splits": 5,
            "n_repeats": 1,
            "random_state": 42
        }
    },
    "data_loader": {
        "type": "TFT_Challengers",
        "args": {
            "data_path": "assets/na1_challengers_12.12.450.4196_matches.pickle",
            "shuffle": true,
            "test_split": 0.15,
            "stratify": true,
            "random_state": 42
        }
    },
    "label_name": "placement",
    "score": "max neg_mean_absolute_error",
    "train_model": true,
    "test_model": true,
    "debug": false,
    "save_dir": "saved/",
    "model_dir": ""
}